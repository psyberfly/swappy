{
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.cargo.loadOutDirsFromCheck": true,
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.highlightRelated.references.enable": true,
        "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.lens.enable": true,
    "rust-analyzer.lens.location": "above_name",
    "rust-analyzer.inlayHints.typeHints.enable": true,
    "rust-analyzer.lens.run": true,
    "rust-analyzer.lens.debug": true,
    "rust-analyzer.cargo.runBuildScripts": true,
    "files.autoSave": "onFocusChange",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "cargo.makeArgs": ["--release"],
    "cargo.buildArgs": ["--release"],
    "cargo.testArgs": ["--release"],
    "cargo.clippyArgs": ["--release"],
    "terminal.integrated.shellArgs.linux": [],
    "terminal.integrated.shellArgs.osx": [],
    "terminal.integrated.shellArgs.windows": []
}
